<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".sendGPS"
    tools:showIn="@layout/activity_send_gps">

    <TextView
        android:id="@+id/longitudeValueGPS"
        android:layout_width="146dp"
        android:layout_height="35dp"
        android:layout_marginEnd="156dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="44dp"
        android:text="0.000"
        app:layout_constraintBottom_toTopOf="@+id/latitudeValueGPS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/latitudeValueGPS"
        android:layout_width="147dp"
        android:layout_height="30dp"
        android:layout_marginBottom="402dp"
        android:layout_marginEnd="155dp"
        android:layout_marginStart="58dp"
        android:text="0.000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/longitudeValueGPS" />

    <TextView
        android:id="@+id/debug"
        android:layout_width="160dp"
        android:layout_height="54dp"
        android:layout_marginBottom="284dp"
        android:layout_marginEnd="132dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="64dp"
        android:text="@string/debug"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/latitudeValueGPS"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>